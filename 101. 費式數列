getFib.h:

void getFib(int N);
int Fib(int n);

getFib.c:

#include "getFib.h"
#include <stdio.h>

void getFib(int N) {
	for (int i = 0; i < N; i++) {
		if (i == 0) {
			printf("%d", Fib(i));
		}
		else {
			printf(" %d", Fib(i));
		}
	}
}

int Fib(int n) {
	if (n == 0) {
		return 0;
	}
	else if (n == 1) {
		return 1;
	}
	else if (n >= 2) {
		return Fib(n - 1) + Fib(n - 2);
	}
}
