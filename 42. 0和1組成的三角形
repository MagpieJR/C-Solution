void main() {
	int N;
	scanf("%d", &N);
	for (int i = 1; i < N + 1; i++) {
		int flag;
		if (i % 2 == 0) {
			flag = 1;
		}
		else {
			flag = 0;
		}
		for (int j = 1; j < i + 1; j++) {
			if (flag == 0) {
				flag = 1;
			}
			else {
				flag = 0;
			}
			printf("%d", flag);
		}
		printf("\n");
	}
}
